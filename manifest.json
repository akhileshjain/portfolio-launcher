{
    "manifest_version": 2,
    "name": "Portfolio Launcher",
    "description": "Quick launch your Portfolio",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]
  }